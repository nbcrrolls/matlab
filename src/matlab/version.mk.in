# put iso and mcr distro in this location for unpacking
ISODISTRO = $(DATA)/$(VERSION)/R$(VERSION)-glnxa64.iso
MCRDISTRO = $(DATA)/$(VERSION)/$(MCR)_R$(VERSION)_glnxa64_installer.zip


RPM.EXTRAS = "Autoprov: 0"
