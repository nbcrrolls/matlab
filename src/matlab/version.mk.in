VERSION.MK.MASTER = $(REDHAT.ROOT)/matlab.mk
VERSION.MK.INCLUDE = matlab.version.mk
include $(VERSION.MK.INCLUDE)

VERSION   = $(MATLABVER)
NAME      = matlab
RELEASE   = 0
DATA      = /state/partition1/data
VARDIR    = /state/partition1/var

